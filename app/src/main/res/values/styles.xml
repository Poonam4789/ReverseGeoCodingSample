<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->


    <style name="MyDialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowBackground">@color/white_trans</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
    </style>

    <!--Chart Tablayout textview style  -->

    <!--Exoplayer Style-->
    <!--Calender theme-->
    <attr name="themeDark" format="boolean">false</attr>

    <declare-styleable name="PDFViewPager">
        <attr name="assetFileName" format="string" /> <!-- Load PDF from Asset -->
        <attr name="pdfUrl" format="string" /> <!-- Load PDF from remote URL -->
        <attr name="scale" format="float" /> <!-- Set initial scale for PDF -->
    </declare-styleable>

    <declare-styleable name="SubsamplingScaleImageView">
        <attr name="src" format="reference" />
        <attr name="assetName" format="string" />
        <attr name="panEnabled" format="boolean" />
        <attr name="zoomEnabled" format="boolean" />
        <attr name="quickScaleEnabled" format="boolean" />
        <attr name="tileBackgroundColor" format="color" />
    </declare-styleable>

</resources>